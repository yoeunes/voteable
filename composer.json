{
    "name"             : "yoeunes/voteable",
    "license"          : "MIT",
    "description"      : "This package helps you to add user based voting system to your model.",
    "homepage"         : "https://github.com/yoeunes/voteable",
    "keywords"         : [
        "laravel",
        "likeable",
        "voteable",
        "like",
        "vote",
        "dislike",
        "up vote",
        "down vote",
        "laravel"
    ],
    "authors"          : [
        {
            "name"    : "Younes Khoubza",
            "email"   : "younes.khoubza@gmail.com",
            "homepage": "https://dalilbook.com",
            "role"    : "Developer"
        }
    ],
    "require"          : {
        "php": ">=7.1",
        "illuminate/database": ">=6",
        "illuminate/support": ">=6"
    },
    "require-dev"      : {
        "laracasts/testdummy": ">=2.3",
        "orchestra/testbench": ">=4.5",
        "phpunit/phpunit"    : ">=8.0"
    },
    "autoload"         : {
        "psr-4": {
            "Yoeunes\\Voteable\\": "src"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev"     : {
        "psr-4": {
            "Yoeunes\\Voteable\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable"    : true,
    "extra"            : {
        "laravel": {
            "providers": [
                "Yoeunes\\Voteable\\VoteableServiceProvider"
            ]
        }
    },
    "config"           : {
        "sort-packages": true
    }
}
